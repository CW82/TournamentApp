<table border="1">
    <thead>
        <tr>
            <th>teamID</th>
            <th>teamName</th>
            <th>region</th>
            <th>playerCount</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {{#each teams}}
        <tr>
            <td>{{teamID}}</td>
            <td>{{teamName}}</td>
            <td>{{region}}</td>
            <td>{{playerCount}}</td>
            <td>
                <button type="button">Delete</button>
                <button type="button">Add</button>
            </td> 
        </tr>
        {{/each}}
    </tbody>
</table>